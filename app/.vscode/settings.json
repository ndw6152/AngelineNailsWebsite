// Workspace settings
{
  // The following will hide the js and map files in the editor
  "files.exclude": {
    "**/node_modules/": true,
    "**/*.js": true,
    "**/*.map": true,
    "**/package-lock.json": true
  },

  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true
}
